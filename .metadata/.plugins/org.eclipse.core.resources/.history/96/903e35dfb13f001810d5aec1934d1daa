
public class Javaoops {

	//Overloading:
	
	public static void main(String[] args) {
	
	    ChildClass ref =new ChildClass();
		ref.method();
		
		ParentClass ref2= new ParentClass();
		ref2.method();
		
		ParentClass ref3 = new ChildClass();
		ref3.method2();
		ref3.eat();
	    System.out.println(ref3.y);
	    
	    InterfaceB ref4 = new ParentClass();
	    ref4.eat();
	
	}


	
	
}
